[run]
source = text_manipulation
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */virtualenv/*
    */site-packages/*
    setup.py
    conftest.py
    test_*.py
    *_test.py
branch = true
parallel = true

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
precision = 2
show_missing = true
skip_covered = false
skip_empty = true

[html]
directory = htmlcov
title = Text Manipulation Tool Coverage Report

[xml]
output = coverage.xml 